  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env DEBUG='express:*' && webpack-dev-server --debug --output-pathinfo --history-api-fallback --hot --inline --progress --colors --port 3000 --open",
    "start2": "cross-env DEBUG='express:*' && webpack-dev-server --progress --open",
    "build": "cross-env NODE_ENV=production webpack --progress -p --colors",
    "build-cdn": "rimraf dist-cdn/ && cross-env NODE_ENV=cdn webpack --progress -p --colors",
    "build-upload-cdn": "cross-env NODE_ENV=cdn webpack --progress -p --colors && ./push-sdkweb.sh",
    "build2": "rimraf dist/ && cross-env NODE_ENV=production webpack --progress -p --colors",
    "update_texts": "node ./scripts/updateDiveLiterals.js",
    "hbbtv-server": "node server.js",
    "hbbtv-server-pro": "cross-env NODE_ENV=production node server.js",
    "upload-to-cdn": "rimraf dist-cdn/ && cross-env NODE_ENV=cdn webpack --progress -p --colors && cross-env NODE_ENV=cdn PUBLICPATH=cdn node server.js && echo 'UPLOADED'",
    "docs": "jsdoc2md *.js > api.md"
  },
